
<!-- Header -->
<%- include('include/_header') %>
<!-- /Header -->

<!-- Main -->
<main class="container">
  <div class="card-columns pt-5">
      <% for(var i = 0; i < result.length; i++) { %>
          <div class="card" style="width: 322px;">
              <h4><%= result[i].ItemName %></h4>
              <img src="Img/<%= result[i].ImagePath %>" style="width:100%;height: 200px;" class="mx-auto">
              <a href="/DetailedMenu?Id=<%= result[i].Id%>" class="btn btn-warning">View All</a>
          </div>
      <% } %>
  </div>
</main>

<!-- /Main -->

<!-- Footer -->
<%- include('include/_footer') %>
<!-- /Footer -->